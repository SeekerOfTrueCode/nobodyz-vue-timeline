{
  "name": "vue-nobodyz-timeline",
  "author": "seekeroftruecode",
  "version": "0.2.1",
  "license": "MIT",
  "private": false,
  "main": "./dist/lib/vue-nobodyz-timeline.common.js",
  "unpkg": "./dist/wc/vue-nobodyz-timeline.min.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle:types": "tsc -p tsconfig.json",
    "build-bundle:library": "vue-cli-service build --target lib --name vue-nobodyz-timeline ./src/index.ts --dest dist/lib",
    "build-bundle:wc": "vue-cli-service build --target wc --name vue-nobodyz-timeline ./src/components/*.vue --dest dist/wc",
    "build-bundle:all": "npm run build-bundle:library && npm run build-bundle:wc",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "files": [
    "dist/*",
    "src/*",
    "types/*.d.ts",
    "*.json",
    "*.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeekerOfTrueCode/vue-nobodyz-timeline.git"
  },
  "keywords": [
    "vue",
    "vue2",
    "vuejs",
    "vuejs2",
    "timeline",
    "day timeline",
    "nobodyz"
  ],
  "dependencies": {
    "core-js": "^3.6.4",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.4.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-typescript": "^4.1.2",
    "@vue/cli-plugin-unit-jest": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-standard": "^5.1.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vue/test-utils": "1.0.0-beta.31",
    "eslint": "^6.2.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.1.2",
    "node-sass": "^4.13.1",
    "resize-observer-polyfill": "^1.5.1",
    "sass-loader": "^8.0.2",
    "typescript": "^3.7.5",
    "vue-template-compiler": "^2.6.11"
  }
}
